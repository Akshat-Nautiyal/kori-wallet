{
  "name": "koriwallet",
  "version": "1.0.0",
  "description": "Kori wallet",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . koriwallet",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "deb64": "electron-installer-debian --src dist/app-linux-x64/ --dest dist/installers/ --arch amd64"
  },
  "author": "kori",
  "license": "ISC",
  "devDependencies": {
    "asar": "^2.0.1",
    "electron": "^5.0.6",
    "electron-builder": "^21.1.1",
    "electron-installer-debian": "^2.0.0",
    "electron-packager": "^14.0.2"
  },
  "electronPackagerConfig": {
    "packageManager": "npm",
    "executableName": "kori"
  },
  "build": {
    "appId": "kori",
    "linux": {
      "category": "wallet"
    },
    "win": {
      "category": "wallet"
    }
  },
  "dependencies": {
    "cookies": "^0.7.3",
    "crypto": "^1.0.1",
    "dpkg": "^1.0.0",
    "express": "^4.17.1",
    "jquery": "^3.5.0",
    "jstorage": "^0.4.8",
    "request": "^2.88.0",
    "update-electron-app": "^1.5.0"
  }
}
